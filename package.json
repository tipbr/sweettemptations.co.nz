{
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:eleventy": "eleventy --serve",
    "dev:css": "tailwindcss -i ./src/styles/main.css -o ./src/styles/output.css --watch",
    "build": "npm-run-all build:*",
